
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Post {
  id     Int    @id @default(autoincrement())
  userId Int
  title  String
  body   String
}

model Contact {
  id        Int     @id @default(autoincrement())
  firstName String
  lastName  String
  email     String
  phone     String?
  message   String
}
